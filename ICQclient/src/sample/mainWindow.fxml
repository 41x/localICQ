<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.mainWindowController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" vgrow="ALWAYS" />
      <RowConstraints maxHeight="24.0" minHeight="7.0" prefHeight="23.0" />
      <RowConstraints minHeight="0.0" prefHeight="48.0" vgrow="ALWAYS" />
      <RowConstraints minHeight="10.0" />
      <RowConstraints maxHeight="120.0" minHeight="79.0" prefHeight="79.0" />
   </rowConstraints>
   <children>
      <TextArea fx:id="logBox" disable="true" editable="false" prefHeight="110.0" prefWidth="269.0" GridPane.hgrow="NEVER" GridPane.rowIndex="4" GridPane.vgrow="ALWAYS" />
      <TextField fx:id="usernameBox" promptText="Имя пользователя" />
      <Button defaultButton="true" mnemonicParsing="false" onAction="#onAddContact" text="Добавить контакт" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
      <VBox fx:id="ContactVBox" prefHeight="300.0" prefWidth="450.0" GridPane.rowIndex="2" />
      <Button mnemonicParsing="false" onAction="#onDeleteContact" prefHeight="25.0" prefWidth="113.0" text="Удалить контакт" GridPane.halignment="CENTER" GridPane.rowIndex="3" />
   </children>
</GridPane>
